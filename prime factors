import java.util.Scanner;
public class Main {
    public static void prime(int n){
        for(int i=2;i<=n;i++){
            while(n%i==0){
                System.out.print(i + " ");
                n/=i;
            }
        }
    }
    public static void main(String[] args) 
    {
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter the number:");
    int n=sc.nextInt();
    prime(n);
    
    }
}
